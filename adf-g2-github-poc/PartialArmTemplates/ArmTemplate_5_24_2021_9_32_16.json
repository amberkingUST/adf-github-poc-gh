{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-g2-github-poc"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/Grp1POCBranch')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "exec stored procedure",
						"type": "SqlServerStoredProcedure",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": true,
							"secureInput": true
						},
						"userProperties": [],
						"typeProperties": {
							"storedProcedureName": {
								"value": "@variables('var')",
								"type": "Expression"
							}
						},
						"linkedServiceName": {
							"referenceName": "sqlserverconnection",
							"type": "LinkedServiceReference",
							"parameters": {
								"Environment": {
									"value": "@concat(pipeline().globalParameters.EnvironmentGP,'sqlserverconnectionstring')",
									"type": "Expression"
								}
							}
						}
					}
				],
				"variables": {
					"var": {
						"type": "String",
						"defaultValue": "abc"
					}
				},
				"annotations": [],
				"lastPublishTime": "2021-05-18T00:21:58Z"
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/sqlserverconnection')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/TestPipeline3')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Set variable1",
						"type": "SetVariable",
						"dependsOn": [],
						"userProperties": [],
						"typeProperties": {
							"variableName": "var2",
							"value": "a"
						}
					}
				],
				"variables": {
					"var2": {
						"type": "String",
						"defaultValue": "ab"
					}
				},
				"annotations": []
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/adfgithubpockv')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureKeyVault",
				"typeProperties": {
					"baseUrl": "https://adf-github-poc-kv.vault.azure.net/"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/sqlserverconnection')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"parameters": {
					"Environment": {
						"type": "string"
					}
				},
				"annotations": [],
				"type": "AzureSqlDatabase",
				"typeProperties": {
					"connectionString": {
						"type": "AzureKeyVaultSecret",
						"store": {
							"referenceName": "adfgithubpockv",
							"type": "LinkedServiceReference"
						},
						"secretName": {
							"value": "@linkedService().Environment",
							"type": "Expression"
						}
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/adfgithubpockv')]"
			]
		}
	]
}