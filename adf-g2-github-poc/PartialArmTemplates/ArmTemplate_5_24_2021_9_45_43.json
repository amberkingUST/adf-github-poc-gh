{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-g2-github-poc"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/Grp1POCBranch')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "exec stored procedure",
						"type": "SqlServerStoredProcedure",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"storedProcedureName": {
								"value": "@variables('var')",
								"type": "Expression"
							}
						},
						"linkedServiceName": {
							"referenceName": "sqlserverconnection",
							"type": "LinkedServiceReference",
							"parameters": {
								"Environment": {
									"value": "@concat(pipeline().globalParameters.EnvironmentGP,'-sqlserverconnectionstring')",
									"type": "Expression"
								}
							}
						}
					}
				],
				"variables": {
					"var": {
						"type": "String",
						"defaultValue": "abc"
					}
				},
				"annotations": [],
				"lastPublishTime": "2021-05-18T00:21:58Z"
			},
			"dependsOn": []
		}
	]
}